{
    "compilerOptions": {
        "allowJs": true,
        "checkJs": false,
        "skipLibCheck": true,
        "sourceMap": false,
        "noEmit": false,
        "target": "ES5",
        "outDir": "lib",
        "moduleResolution": "Node",
        "baseUrl": "src",
        "rootDir": "src",
        "strict": true,
        "esModuleInterop": true,
        "lib": ["ES2015"]
    },
    "exclude": [
        "node_modules",
        "lib",
        "test"
    ],
}